{
    "name": "lootvgo-backend",
    "version": "1.0.1",
    "description": "Backend application for LootVGO.com",
    "main": "app.js",
    "scripts": {
        "start": "NODE_ENV=development npm run db-setup && pm2 startOrRestart ecosystem-dev.json --max-memory-restart 8G && pm2 save",
        "start-windows": "cross-env NODE_ENV=localhost nodemon app.js",
        "db-setup": "cross-env NODE_ENV=development node db-setup.js",
        "db-setup-production": "cross-env NODE_ENV=production node db-setup.js",
        "db-create": "cross-env NODE_ENV=development node db-create.js",
        "db-fix": "cross-env NODE_ENV=development node db-fix.js",
        "set-rank": "cross-env NODE_ENV=production node set-ranks.js",
        "start-production": "NODE_ENV=production npm run db-setup && pm2 startOrRestart ecosystem.json --max-memory-restart 8G && pm2 save"
    },
    "author": "LootVGO",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.9.0",
        "async": "2.6.1",
        "async-cache": "1.1.0",
        "bluebird": "^3.5.3",
        "cookie-parser": "1.4.3",
        "express": "4.16.3",
        "express-session": "1.15.6",
        "express-socket.io-session": "1.3.4",
        "helmet": "3.12.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "node-2fa": "^1.1.2",
        "node-schedule": "^1.3.2",
        "opskins-tradeoffer-manager": "0.6.1",
        "passport": "0.4.0",
        "passport-oauth2": "1.4.0",
        "passport-steam": "1.0.10",
        "request": "2.87.0",
        "request-promise": "^4.2.2",
        "rethinkdbdash": "2.3.31",
        "serve-static": "1.13.2",
        "session-rethinkdb": "2.0.1",
        "socket.io": "2.1.1"
    }
}
